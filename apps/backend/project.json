{
  "name": "backend",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go run main.go",
        "cwd": "apps/backend"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build -o dist/backend main.go",
        "cwd": "apps/backend"
      }
    }
  }
}